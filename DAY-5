import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        int i,N=2,result;
        for(i=1;i<=10;i++){
            result= N*i;
           System.out.println("%d * %d= "+N+i+result); 
            
        }
    }
}
